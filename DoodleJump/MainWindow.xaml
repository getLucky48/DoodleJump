<Window x:Class="DoodleJump.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:DoodleJump"
        mc:Ignorable="d"
        Title="Launcher" Height="349" Width="372.5" ResizeMode="NoResize" WindowStartupLocation="CenterScreen">
    <Grid>
        <Label x:Name="Label_Title" Content="DoodleJump" HorizontalAlignment="Left" Margin="10,24,0,0" VerticalAlignment="Top" FontSize="36" FontFamily="Segoe UI Black" Width="346" HorizontalContentAlignment="Center" VerticalContentAlignment="Center"/>
        <Button x:Name="Button_Start" Content="СТАРТ" Margin="111,121,111,0" VerticalAlignment="Top" Height="48" FontSize="20" FontFamily="Segoe UI Black" Click="Button_Start_Click"/>
        <Button x:Name="Button_Exit" Content="ВЫХОД" Margin="111,208,111,0" VerticalAlignment="Top" Height="48" FontSize="20" FontFamily="Segoe UI Black" Click="Button_Exit_Click"/>

    </Grid>

</Window>
